# .editorconfig file

# top-most EditorConfig file
root = true

# Set charset to UTF-8
charset = utf-8

# Indentation style
[*.{md,html,css,js}]
indent_style = tab
indent_size = 2

# Trim trailing whitespace
trim_trailing_whitespace = true

# Insert final newline
insert_final_newline = true

# End of file
